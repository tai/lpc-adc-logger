
(rules PCB lpc-adc-logger
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 4)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.5)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.7)
    )
  )
  (rule
    (width 200.0)
    (clear 200.2)
    (clear 100.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_500:300_um"
    (shape
      (circle F.Cu 500.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 500.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_500:300_um" "Via[0-1]_500:300_um" default
  )
  (via 
    "Via[0-1]_500:300_um-kicad_default" "Via[0-1]_500:300_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_500:300_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_500:300_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 200.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    GND VCC "Net-(D1-Pad1)" "Net-(D1-Pad2)" "Net-(J1-Pad1)" "Net-(J1-Pad2)" "Net-(J1-Pad5)" "Net-(J1-Pad7)"
    "Net-(J1-Pad8)" "Net-(J1-Pad9)" "Net-(J1-Pad10)" "Net-(J1-Pad11)" "Net-(J1-Pad12)" "Net-(J1-Pad13)" "Net-(J2-Pad2)" "Net-(J2-Pad3)"
    "Net-(J3-Pad2)" "Net-(J3-Pad3)" "Net-(J4-Pad1)" "Net-(J4-Pad3)" "Net-(J4-Pad5)" "Net-(J4-Pad7)" "Net-(R1-Pad1)" "Net-(R2-Pad1)"
    "Net-(R3-Pad1)" "Net-(R4-Pad1)" "Net-(R10-Pad1)" "Net-(SW1-Pad1)" "Net-(SW3-Pad1)" "Net-(U1-Pad18)" "Net-(U1-Pad5)" "Net-(U1-Pad19)"
    "Net-(U1-Pad20)" "Net-(U1-Pad24)" "Net-(U1-Pad25)" "Net-(U1-Pad13)" "Net-(U1-Pad27)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 200.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)